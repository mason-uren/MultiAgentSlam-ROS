set(PACKAGE_TARGET_NAME "Interfaces")

get_filename_component(${PACKAGE_TARGET_NAME}_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
include(CmakeFindDependencyMacro)

list(APPEND CMAKE_MODULE_PATH ${${PACKAGE_TARGET_NAME}_CMAKE_DIR})

# List dependency (ie find_package())

list(REMOVE_AT CMAKE_MODULE_PATH -1)

if (NOT TARGET ${PACKAGE_TARGET_NAME}::${PACKAGE_TARGET_NAME})
    include("${${PACKAGE_TARGET_NAME}_CMAKE_DIR}/${PACKAGE_TARGET_NAME}Targets.cmake")
endif ()

set(${PACKAGE_TARGET_NAME}_LIBRARIES ${PACKAGE_TARGET_NAME}::${PACKAGE_TARGET_NAME})